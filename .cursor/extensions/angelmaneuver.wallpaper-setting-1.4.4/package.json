		"ts-patch": "^3.3.0",
		"typescript": "^5.7.2",
		"typescript-transform-paths": "^3.5.3",
		"webpack": "^5.97.1",
		"webpack-cli": "^6.0.1"
	},
	"packageManager": "yarn@1.22.22",
